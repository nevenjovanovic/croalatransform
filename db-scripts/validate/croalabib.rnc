        | tei_model.noteLike
        | tei_model.biblLike)*),
    tei_att.global.attributes,
    tei_att.datable.attributes,
    tei_att.editLike.attributes,
    tei_att.naming.attributes,
    tei_att.typed.attributes,
    empty
  }
tei_att.global.analytic.attributes =
  tei_att.global.analytic.attribute.ana
tei_att.global.analytic.attribute.ana =
  
  ## (analysis) indicates one or more elements containing interpretations of the element on which the ana attribute appears.
  attribute ana {
    list { tei_data.pointer+ }
  }?
tei_s =
  
  ## (s-unit) contains a sentence-like division of a text. [17.1. Linguistic Segment Categories 8.4.1. Segmentation]
  element s {
    ((text
      | tei_model.gLike
      | tei_model.global
      | tei_foreign
      | tei_term
      | tei_title
      | tei_hi
      | tei_address
      | tei_affiliation
      | tei_date
      | tei_time
      | tei_height
      | tei_num
      | tei_name
      | tei_orgName
      | tei_persName
      | tei_addName
      | tei_roleName
      | tei_country
      | tei_district
      | tei_placeName
      | tei_settlement
      | tei_state
      | tei_trait
      | tei_idno
      | tei_abbr
      | tei_am
      | tei_ex
      | tei_expan
      | tei_subst
      | tei_add
      | tei_corr
      | tei_damage
      | tei_del
      | tei_handShift
      | tei_mod
      | tei_redo
      | tei_restore
      | tei_retrace
      | tei_sic
      | tei_supplied
      | tei_surplus
      | tei_unclear
      | tei_undo
      | tei_dimensions
      | tei_locus
      | tei_origDate
      | tei_origPlace
      | tei_ref
      | tei_c
      | tei_cl
      | tei_m
      | tei_pc
      | tei_phr
      | tei_seg
      | tei_w)*)
    >> sch:pattern [
         id = "croalabib-s-noNestedS-constraint-11"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:s"
           "\x{a}" ~
           "               "
           sch:report [
             test = "tei:s"
             "You may not nest one s element within\x{a}" ~
             "      another: use seg instead"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    tei_att.global.attributes,
    tei_att.segLike.attributes,
    tei_att.typed.attributes,
    empty
  }
tei_cl =
  
  ## (clause) represents a grammatical clause. [17.1. Linguistic Segment Categories]
  element cl {
