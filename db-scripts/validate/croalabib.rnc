     | tei_model.gLike
     | tei_seg
     | tei_m
     | tei_c
     | tei_model.global)*,
    tei_att.global.attributes,
    tei_att.segLike.attributes,
    tei_att.typed.attributes,
    
    ## supplies the morpheme's base form.
    attribute baseForm { tei_data.word }?,
